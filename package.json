{
    "name": "@cmmv/blueprint",
    "version": "0.0.1",
    "description": "CMMV frontend script",
    "private": true,
    "scripts": {
        "build": "npm run build:compile && npm run build:core",
        "build:packages": "rm -rf ./packages/**/*.tsbuildinfo && NODE_ENV=build tsc -b -v packages",
        "build:compile": "npm run clean:compile && ts-node -r tsconfig-paths/register ./src/compile.ts",
        "build:core": "tsc && npm run copy:misc",
        "prebuild:prod": "npm run clean",
        "clean:tsfiles": "find ./src -type f \\( -name '*.d.ts' -o -name '*.js' \\) -exec rm -f {} +",
		"clean:packages": "find ./packages -name 'node_modules' -prune -o -type f \\( -name '*.d.ts' -o -name '*.js' \\) -exec rm -f {} +",
		"clean:compile": "rm -rf ./node_modules/@cmmv/blueprint && rm -rf ./node_modules/@cmmv/nodes",
        "clean": "npm run clean:tsfiles && npm run clean:packages > /dev/null 2>&1",
        "test": "npm run build:test && npm run packages:move && mocha && npm run clean",
		"test:dev": "mocha -w --watch-files \"packages\" packages/**/*.spec.js",
        "copy:misc": "ts-node ./src/copy-misc.ts",
        "playground": "ts-node ./src/playground.ts",
        "playground:prod": "NODE_ENV=prod node ./dist/playground.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "release:lerna": "./node_modules/.bin/lerna publish --force-publish --exact -m \"chore: chore(@cmmv) publish %s release\"",
		"release": "npm run build:packages && npm run packages:move && npm run release:lerna && npm run clean",
        "prerelease": "gulp copy-misc",
        "packages:move": "gulp move",
        "prepare": "husky"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrehrferreira/cmmv-blueprint.git"
    },
    "keywords": [
        "cmmv",
        "blueprint",
        "nodes",
        "graph"
    ],
    "author": "Andre Ferreira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andrehrferreira/cmmv-blueprint/issues"
    },
    "homepage": "https://github.com/andrehrferreira/cmmv-blueprint#readme",
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-angular": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@types/chai": "^4.3.19",
        "@types/chai-as-promised": "^8.0.0",
        "@types/jest": "^29.5.13",
        "@types/mocha": "^10.0.8",
        "@types/node": "^22.5.1",
        "@types/sinon": "^17.0.3",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "chai": "^5.1.1",
        "chai-as-promised": "^8.0.0",
        "chalk": "^5.3.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "delete-empty": "^3.0.0",
        "enquirer": "^2.4.1",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.30.0",
        "execa": "^9.3.1",
        "gulp": "^5.0.0",
        "gulp-clean": "^0.4.0",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "jsdom": "^25.0.0",
        "jsdom-global": "^3.0.2",
        "lerna": "^8.1.8",
        "lerna-changelog": "^2.2.0",
        "lint-staged": "^15.2.10",
        "mocha": "^10.7.3",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "semver": "^7.6.3",
        "sinon": "^19.0.2",
        "supertest": "^7.0.0",
        "terser": "^5.31.6",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.2"
    },
    "dependencies": {
        "@cmmv/core": "^0.3.3",
        "fast-glob": "^3.3.2",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "uid": "^2.0.2",
        "uuid": "^10.0.0"
    }
}
