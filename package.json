{
    "name": "@cmmv/blueprint",
    "version": "0.0.1",
    "description": "CMMV frontend script",
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "private": false,
    "scripts": {
        "build": "npm run build:compile && npm run build:core",
        "build:compile": "ts-node ./src/compile.ts",
        "build:core": "tsc && npm run copy:misc",
        "copy:misc": "ts-node ./src/copy-misc.ts",
        "playground": "ts-node ./src/playground.ts",
        "playground:prod": "NODE_ENV=prod node ./dist/playground.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "release": "node scripts/release.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrehrferreira/cmmv-blueprint.git"
    },
    "keywords": [
        "cmmv",
        "blueprint",
        "nodes",
        "graph"
    ],
    "author": "Andre Ferreira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andrehrferreira/cmmv-blueprint/issues"
    },
    "homepage": "https://github.com/andrehrferreira/cmmv-blueprint#readme",
    "devDependencies": {
        "@types/chai": "^4.3.19",
        "@types/mocha": "^10.0.7",
        "@types/node": "^22.5.1",
        "chai": "^5.1.1",
        "chalk": "^5.3.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "cross-env": "^7.0.3",
        "enquirer": "^2.4.1",
        "execa": "^9.3.1",
        "jsdom": "^25.0.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^10.7.3",
        "prettier": "^3.3.3",
        "semver": "^7.6.3",
        "terser": "^5.31.6",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.2"
    },
    "dependencies": {
        "@cmmv/core": "^0.3.3",
        "fast-glob": "^3.3.2",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "uid": "^2.0.2",
        "uuid": "^10.0.0"
    }
}
